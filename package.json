{
  "name": "back-end",
  "version": "1.0.0",
  "description": "redeeming soles project back-end",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --forceExit --runInBand",
    "dbon": "mkdir -p ./db && mongod -v --dbpath ./db",
    "dboff": "killall mongod",
    "build": "babel src -d build",
    "start": "babel src -d build && node index.js",
    "coverage-badge": "coverage-badger -r coverage/clover.xml -d coverage/",
    "coverage": "npm test -- --coverage && npm run coverage-badge",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/__test__/lib/test.env.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedeemingSoles/back-end.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RedeemingSoles/back-end/issues"
  },
  "homepage": "https://github.com/RedeemingSoles/back-end#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "crypto": "^1.0.1",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "http-errors": "^1.6.3",
    "jest": "^23.1.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.1.5",
    "superagent": "^3.8.3",
    "winston": "^3.0.0"
  },
  "dependencies": {
    "cors": "^2.8.4"
  }
}
